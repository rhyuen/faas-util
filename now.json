{
    "version": 2,
    "name": "royuutil",
    "alias": ["royuutil"],
    "builds": [{
        "src": "functions/*.js",
        "use": "@now/node"
    }, {
        "src": "public/index.html",
        "use": "@now/static"
    }],
    "routes": [{
        "src": "/api/sha2",
        "dest": "/functions/sha2.js"
    }, {
        "src": "/api/(.*)",
        "dest": "/functions/$1.js"
    }, {
        "src": "/(.*)",
        "dest": "public/$1"
    }],
    "public": true
}